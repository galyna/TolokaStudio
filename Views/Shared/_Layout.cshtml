@using SumkaWeb.Common.Extensions
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/style/sumka_content.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery/modernizr-1.7.min.js")" type="text/javascript"></script>
</head>
<body class="no_js_body">
    <!--Block Menu-->
    <script type="text/javascript">
        document.body.className = "";
    </script>
    <div class="menu_wrap">
        <div class="container">
            <div class="menu_top">
                <div class="menu_top_list">
                    @Html.Menu(
                    "<ul class='navigation'>" +
                       " <li class='first'><a class='link typ1' href='" + "/Home" + "'>Головна</a></li>" +
                        "<li class='typ2'><a href='" + "/Store" + "' class='typ2'>Послуги</a></li>" +
                        "<li><a class='typ3' href='" + "/Product" + "'>Товари</a></li>" +
                                //" <li><a class='typ4' href='/Store'>Автори</a></li>" +
                                " <li class='last'><a class='typ6' href='http://vk.com/public43703617'>Контакти</a></li>" +
                   " </ul>")
                    <div class="clear">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--Block Menu ends-->
    <div class="wrap_site">
        <div class="container">
            <div class="row">
                <div class="span24 offset24">
                    @Html.Partial("_LogOnPartial")
                </div>
            </div>
        </div>
        @RenderBody()
    </div>
    <div class="footer">
        <div class="container">
            <div class="menu_and_social_wrap">
                <div class="row">
                    <div class="span19">
                        <div class="menu_footer">
                            @Html.Menu(
                          "<ul class='navigation'>" +
                                                     
                                                    " <li class='first'><a class='link typ1' href='" + "/Home" + "'>Головна</a></li>" +
                                 "<li class='typ2'><a href='" + "/Store" + "' class='typ2'>Послуги</a></li>" +
                                 "<li><a class='typ3' href='" + "/Product" + "'>Товари</a></li>" +
                                         " <li><a class='typ4' href='/Store'>Автори</a></li>" +
                                         " <li class='last'><a class='typ6' href='http://vk.com/public43703617'>Контакти</a></li>" +
                            "</ul>")
                            <div class="clear">
                            </div>
                        </div>
                    </div>
                    <div class="span5">
                        <div class="social_link_wrap">
                            <div class="social_link_text">

                                Ми ще тут:</div>
                            <div class="social_icons">
                                <ul>
                                   @* <li class="facebook"><a href="#"></a></li>
                                    <li class="twitter"><a href="#"></a></li>
                                    <li class="linkedin"><a href="#"></a></li>*@
                                    <li class="youtube"><a href="http://www.youtube.com/watch?v=JkePvKqQ-g0&feature=BFa&list=PL743A5BA25DFCEC99"></a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="copyright_and_min_menu_wrap">
                <div class="row">
                    <div class="span19">
                        <div class="copyright">
                           Толока Студіо
                        </div>
                    </div>
                    <div class="span5">
                       
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--********************
    JavaScrip all links
**********************-->
    <script type="text/javascript" src="http://platform.twitter.com/widgets.js"></script>
    <script src="@Url.Content("~/Content/assets/js/google-code-prettify/prettify.js")"></script>
    <script src="@Url.Content("~/Content/js/scroller_load_img.js")"></script>
    <!--Menu full width-->
    <script src="@Url.Content("~/Scripts/sumkaScripts/menu_full_width.js")" type="text/javascript"></script>
    <script type="text/javascript">

        window.onload = window.onresize = function (event) {

            /*Menu top*/
            MenuResizer.resize(".menu_top", ".menu_top_list", true);

        }
    </script>
    <!--Menu full width ends-->
</body>
</html>
